<Window x:Class="ChessClient.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChessClient"
        mc:Ignorable="d"
        Title="Register" Height="450" Width="800">
    <Grid>
        <Button x:Name="addbtn" Content="Add New" HorizontalAlignment="Left" Margin="681,24,0,0" VerticalAlignment="Top" Width="75" Click="addbtn_Click"/>
        <Button x:Name="chnbtn" Content="Change" HorizontalAlignment="Left" Margin="681,77.196,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.307,0.717" Click="chnbtn_Click"/>
        <Button x:Name="delbtn" Content="Delete" HorizontalAlignment="Left" Margin="681,136.353,0,0" VerticalAlignment="Top" Width="75" Click="delbtn_Click"/>
       
        <ListBox x:Name="PlayersList" HorizontalAlignment="Left" Height="258.333" Margin="23.667,24,0,0" VerticalAlignment="Top" Width="594.001">
          
        </ListBox>
        <Button Content="Close" HorizontalAlignment="Left" Margin="681,366.999,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.6,0.55" Click="Button_Click"/>
    </Grid>
</Window>
